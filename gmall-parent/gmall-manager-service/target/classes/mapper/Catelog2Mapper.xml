<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xfp.gmall.manager.mapper.Catelog2Mapper">

    <select id="getCateLog2" parameterType="string"
            resultType="com.xfp.gmall.manager.bean.PmsBaseCatalog2">
        select pc2.*   from  pms_base_catalog2 pc2
        where pc2.catalog1_id=#{catalog1_id}
    </select>


</mapper>